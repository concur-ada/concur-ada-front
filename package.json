      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(@ui5|lit-html)).*\\.js$"
    ]
  },
  "devDependencies": {
    "husky": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  }
